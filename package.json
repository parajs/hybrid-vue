{
  "name": "vant-vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env _ENABLE_VCONSOLE=true vue-cli-service serve",
    "serve:mock": "cross-env _ENABLE_VCONSOLE=true _ENABLE_MOCK=true vue-cli-service serve",
    "serve:app": "cross-env _APIURL=url _ENABLE_VCONSOLE=true vue-cli-service serve",
    "build": "cross-env _APIURL=url _REMOVE_CONSOLE=true vue-cli-service build",
    "report": "cross-env _APIURL=url _REMOVE_CONSOLE=true  vue-cli-service build --report",
    "staging": "cross-env _APIURL=url _ENABLE_VCONSOLE=true vue-cli-service build --mode staging",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "cz": "npm run log && git add . && git cz",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.{js,vue}\" --locales \"./src/locales/**/*.js\"",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "dayjs": "^1.8.33",
    "js-cookie": "^2.2.1",
    "nprogress": "^0.2.0",
    "register-service-worker": "^1.7.1",
    "vant": "^2.10.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^3.0.9",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "mockjs": "^1.1.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^1.19.1",
    "right-pad": "^1.0.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "svg-sprite-loader": "^5.0.0",
    "vconsole-webpack-plugin": "^1.5.2",
    "vue-cli-plugin-commitlint": "~1.0.12",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
